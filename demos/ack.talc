#!/usr/bin/talc

# http://en.wikipedia.org/wiki/Ackermann_function
function ack(a, b: int) : int {
  if (a == 0) {
    return b+1;
  } else if (b == 0) {
    return ack(a-1, 1);
  }
  return ack(a-1, ack(a, b-1));
}

for (i := 0; i < 4; ++i) {
  for (j := 0; j < 5; ++j) {
    puts("ack(", i, ", ", j, ") = ", ack(i, j));
  }
}
