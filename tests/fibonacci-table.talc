#!/usr/bin/talc

max_count := 35;

# http://en.wikipedia.org/wiki/Fibonacci_number
function fib(n:int):int {
  n0 := 0;
  n1 := 1;
  for (i := 0; i < n; ++i) {
    n2 := n0 + n1;
    n0 = n1;
    n1 = n2;
  }
  return n0;
}

for (i := 0; i < max_count; ++i) {
  print(i, "\t", fib(i), "\n");
}
exit(0);
